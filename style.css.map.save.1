**/\n/*   TAGLINE FOR ARTICLE POSTS AND PAGES        */\n/*   Small gray type at the end of an article   */\n\n.bbg__tagline,\n.bbg__tagline p,\n.bbg-tagline,\n.bbg-tagline p {\n\tcolor: $color-gray-medium;\n\tfont: {\n\t\tfamily: $font-sans;\n\t\tsize: 1.5rem;\n\t}\n\tline-height: 1.7;\n\tmargin-bottom: $site-margins;\n\n\ta {\n\t\tcolor: $color-gray-dark;\n\t\ttext-decoration: underline;\n\t}\n}\n\n.bbg__tagline-link {\n\tdisplay: block;\n\tmargin: 0 auto $site-margins auto;\n\tmax-width: $article-max-width;\n}\n\n.bbg__article-footer {\n\tmargin-top: $site-margins-large;\n}\n\n/* At the end of a post, add a small tagline credit for authors\ne.g. on the intern testimonial page */\n.bbg__post__author-tagline {\n\ttext-align: right;\n\tmargin-top: 5rem;\n}\n\n/** Read more link **/\n.bbg__read-more {\n\tcolor: $color-primary;\n\tfont: {\n\t\tfamily: $font-sans;\n\t\tsize: 80%;\n\t\tweight: bold;\n\t\ttransform: uppercase;\n\t}\n\tmargin-left: $site-margins-small;\n}","// Custom bbg grid stuff to set how/when columns will split.\n// Ideally this would be setup like this:\n// https://medium.com/@pstonier/using-bourbon-neat-for-css-grid-structure-with-semantic-markup-cd1b13d48dc1#.it31zyc4u\n// But USWDS supersedes Neat's mixin for media (which would accept min, max and column count)\n\n// CONVERT GRIDS (SMALL, MEDIUM, LARGE) FOR SCREEN SIZES\n\n.kr-five-twelfths {@include span-columns(12);}\n.kr-seven-twelfths {@include span-columns(12);}\n\n.bbg-grid--2-3-3 {@include span-columns(6); margin-right: $bbg-grid-gutter;}\n.bbg-grid--2-3-3:nth-child(2n) {clear: right; margin-right: 0;}\n\n@include media($medium-screen) {\n\t.kr-five-twelfths {@include span-columns(5);}\n\t.kr-seven-twelfths {@include span-columns(7);}\n\n\t.bbg-grid--1-2-2 {@include span-columns(6); margin-right: 0;}\n\t.bbg-grid--1-2-2:nth-child(2n+1) {clear: left; margin-right: $bbg-grid-gutter;}\n\n\t.bbg-grid--1-2-3 {@include span-columns(6); margin-right: 0;}\n\t.bbg-grid--1-2-3:nth-child(2n+1) {clear: left; margin-right: $bbg-grid-gutter;}\n\n\t.bbg-grid--1-3-3 {@include span-columns(4); margin-right: 2%; clear:none;}\n\t.bbg-grid--1-3-3:nth-child(2n) {margin-right: 2%;}\n\t.bbg-grid--1-3-3:nth-child(3n) {margin-right: 0; clear: right;}\n\n\t.bbg-grid--2-3-3 {@include span-columns(4); clear: none; margin-right: $bbg-grid-gutter;}\n\t.bbg-grid--2-3-3:nth-child(2n) {margin-right: $bbg-grid-gutter;}\n\t.bbg-grid--2-3-3:nth-child(3n) {clear: right; margin-right: 0;}\n}\n\n@include media($medium-large-screen) {\n\t.bbg-grid--1-1-1-2 {@include span-columns(6); clear: none; margin-right: 0;}\n\t.bbg-grid--1-1-1-2:nth-child(2n+1) {clear: left; margin-right: $bbg-grid-gutter;}\n\n\t.bbg-grid--1-2-3 {@include span-columns(4);margin-right: 2%; clear:none;}\n\t.bbg-grid--1-2-3:nth-child(2n+1) {clear: none; margin-right: 2%;}\n\t.bbg-grid--1-2-3:nth-child(3n) {clear: none; margin-right: 0;}\n\t.bbg-grid--1-2-3:nth-child(3n+1) {clear: left;}\n}","// Temporary patch for gaps in CSS as USWDS updates their styles\nimg {\n\theight: auto;\n}\n\n// fix relative img links for background-images in the footer\n// from: ../img/\n// to: img/\n\n\nul.usa-footer-primary-content,\nli.usa-footer-primary-content,\nli.usa-footer-primary-content {\n\tborder-top: 1px solid #CCC;\n\n\t@include media($medium-screen) {\n\t\tborder: none;\n\t}\n\n\t&:last-child {\n\t\tborder-bottom: 1px solid #CCC;\n\n\t\t@include media($medium-screen) {\n\t\t\tborder-bottom: none;\n\t\t}\n\t}\n}\n\n\n//Switching padding for margin to make 3 dropdowns fit in grid.\n.usa-input-grid {\n\t@include media($medium-screen) {\n\t\tmargin-right: 0; //5%;\n\t\tpadding-right: 0;\n\t}\n}\n/*\n.usa-input-grid:last-of-type {\n\t@include media($medium-screen) {\n\t\tmargin-right: 0;\n\t}\n}\n*/\n\n//Not sure we use this on the site now\n.usa-input-grid-small {\n\t@include media($medium-screen) {\n\t\twidth: 30%;\n\t}\n}\n\n\n\n.usa-grid {\n  padding: 0 2rem;\n}\n\n\n//Primary footer category links should have a hand cursor on hover\n.usa-footer-big nav .usa-footer-primary-link {\n\tcursor: pointer;\n\n\t@include media($medium-screen) {\n\t\tcursor: default;\n\t}\n}\n\n\n//Should this be styled with a custom .bbg class?\n.usa-font-lead {\n\tfont: {\n\t\tsize: $lead-font-size * .8;\n\t\tweight: 300;\n\t}\n\n\t@include media($medium-screen) {\n\t\tfont-size: $lead-font-size;\n\t\tline-height: 2.5;\n\t}\n}\n\ninput,\ninput[type=\"text\"],\ninput[type=\"email\"],\ninput[type=\"password\"],\ninput[type=\"url\"],\ninput[type=\"tel\"],\ninput[type=\"number\"],\ninput[type=\"search\"],\ninput[type=\"file\"],\ninput[type=\"date\"],\ninput[type=\"datetime-local\"],\ninput[type=\"month\"],\ninput[type=\"time\"],\ninput[type=\"week\"],\ntextarea,\nselect {\n\tborder: 1px solid #CCC;\n}\n\nform select:focus {\n\tbackground-color: $color-gray-cool-light;\n}\n\n.usa-social-links a {\n\tdisplay: inline-block;\n}\n\n.screen-reader-text {\n\t@extend .usa-sr-only;\n}\n\n\n\nul,\nol {\n\tli:before {\n\t\twidth: 1.2rem;\n\t}\n}",".bbg-header__container {\n\t@include clearfix();\n\tpadding-right: $site-margins-large;\n\t&:focus {background: none;}\n}\n\n.bbg-header__container--home {\n\t@extend .bbg-header__container;\n\tmargin-bottom: 0;\n}\n\n.bbg-header__link {\n\tpadding: 0;\n\t&:hover {\n\t\ttext-decoration: none;\n\t\t.bbg-header__site-title {color: $color-cool-blue-light;}\n\t}\n\t&:focus {background: none;}\n}\n\n.bbg-header__logo {\n\tfloat: left;\n\theight: $site-title-font-size * 1.3;\n\tmargin-right: 1rem;\n}\n\n.bbg-header__site-title {\n\tclear: none;\n\tdisplay: inline-block;\n\tfont: {\n\t\tfamily: $font-sans !important;\n\t\tsize: $site-title-font-size / 2;\n\t}\n\tline-height: 1;\n\tmargin: 0;\n\tpadding-top: .5rem;\n}\n\n.bbg-header__site-title--breakline {display: block;}\n\n.header-feature {\n\t@include grid-push(2);\n}\n\n// MEDIA QUERIES\n@include media($small-screen) {\n\t.bbg-header__logo {height: $site-title-font-size * 1.75;}\n\t.bbg-header__site-title {\n\t\tfont-size: $site-title-font-size /1.5;\n\t\tpadding-top: $site-margins-small;\n\t}\n}\n\n@include media($medium-screen) {\n\t.bbg-header__link {\n\t\tfont-size: 8rem;\n\t\tline-height: 0;\n\t}\n\t.bbg-header__logo {height: $site-title-font-size * 2.2;}\n\t.bbg-header__site-title {\n\t\tfont-size: $site-title-font-size;\n\t\tpadding-top: .2rem;\n\t}\n}\n\n@include media($medium-large-screen) {\n\t.bbg-header__container {margin-bottom: 0;}\n\t.bbg-header__link {font-size: 5rem;}\n\t.bbg-header__site-title {\n\t\tfont-size: 5rem;\n\t\tpadding-top: 1.5rem;\n\t}\n\t.bbg-header__site-title--breakline {display: inline-block;}\n}\n\n\n","#neat-container {\n\t@include grid-container;\n\t\n}","/**\n * CH 02 - 2- and 3-COLUMN PAGE STRUCTURE\n * CH 03 - FOOTER STYLES\n * CH 04 - EXCERPT STYLES\n * CH 05 - SITEWIDE ALERT STYLES\n * CH 06 - Misc post/page styles\n**/\n\n\n/***************************************************/\n/*    CH 02  2- and 3-COLUMN PAGE STRUCTURE       */\n/*    3-column: used on most posts and many pages  */\n/*    2-column: used on expanded profiles e.g. CEO */\n\n.bbg__article-header {\n    @include clearfix();\n    margin: 0 auto $site-margins auto;\n    max-width: 600px;\n\n    @include media($medium-large-screen) {\n        max-width: none;\n    }\n}\n\n.bbg__article-header__banner {\n    background: {\n        size: cover;\n        position: center center; //default positioning, maybe be set in post.\n    }\n    height: 250px;\n    width: 100%;\n\n    @include media($medium-large-screen) {\n        height: 400px;\n    }\n}\n\n\n.bbg__article-content {\n    margin-bottom: $site-margins;\n\n    li {\n        font-family: $font-serif;\n    }\n}\n\n.bbg__article-header__banner--profile {\n    @extend .bbg__article-header__banner;\n    margin-bottom: 0;\n}\n\n\n.bbg__article-sidebar {\n    font-family: $font-sans;\n    margin: 0 auto;\n    max-width: $article-max-width;\n\n    p,\n    ul,\n    ol {\n        font-family: $font-sans; //font-size: 90%;\n        //line-height: 1.5;\n        margin-top: 0;\n    }\n\n    .bbg__article-sidebar__tagline {\n        color: $color-gray-medium;\n        font-size: 90%;\n        margin: {\n            top: 0;\n            bottom: $site-margins-medium;\n        }\n    }\n\n    .bbg__article-sidebar__emphasis--italic {\n    \tcolor: $color-gray;\n    \tfont: {\n    \t\tsize: $base-font-size;\n    \t\tstyle: italic;\n    \t}\n    }\n\n    .bbg__article-sidebar__promo-photo {\n        float: left;\n        margin-right: $site-margins-medium;\n        max-width: 140px;\n        width: 50%;\n    }\n\n    .bbg__sidebar__photo-caption {\n        /* font-size: 1.3rem; */\n        font-weight: 300;\n        line-height: 1.7;\n        margin-top: 1rem;\n    }\n\n    div {\n        margin-bottom: $site-margins;\n    }\n\n    .bbg__sidebar__download {\n        @include clearfix();\n        margin-bottom: $site-margins;\n\n        @include media($small-screen) {\n            margin-bottom: $site-margins-small;\n        }\n\n        @include media($medium-large-screen) {\n            margin-bottom: $site-margins;\n        }\n    }\n\n    .bbg__sidebar__download__title {\n        clear: none;\n\n        @include media($small-screen) {\n            font-size: 2rem;\n        }\n\n        @include media($medium-large-screen) {\n            clear: both;\n            font-size: 1.5rem;\n        }\n    }\n\n    .bbg__sidebar__download__description {\n        margin-top: 0;\n    }\n\n    .bbg__sidebar__download__thumbnail {\n        border: solid 1px $color-gray-lighter;\n        float: left;\n        margin-right: 1rem;\n        width: 50%;\n\n        @include media($medium-screen) {\n            display: none;\n        }\n\n        @include media($medium-large-screen) {\n            display: block;\n            float: none;\n            margin-right: 0;\n            width: 100%;\n        }\n    }\n\n    .bbg__quotation {\n\n        @include media($medium-screen) {\n            .bbg__quotation-text--large {\n                font-size: 2.5rem;\n                font-weight: 100;\n            }\n\n            .bbg__quotation-attribution__text {\n                display: block;\n                max-width: none;\n\n                .bbg__quotation-attribution__name {\n                    font-size: 1.4rem;\n                    text-align: center;\n                }\n\n                .bbg__quotation-attribution__credit {\n                    font-size: 1.1rem;\n                    text-align: center;\n                }\n            }\n        }\n\n        @include media($medium-large-screen) {\n            .bbg__quotation-text--large {\n                font-size: 2.1rem;\n                font-weight: 100;\n            }\n\n            .bbg__quotation-attribution__text {\n\n                .bbg__quotation-attribution__name {\n                    font-size: 1.6rem;\n                }\n\n                .bbg__quotation-attribution__credit {\n                    font-size: 1.2rem;\n                }\n            }\n        }\n    }\n}\n\n.bbg__article-sidebar__list--labeled {\n    @include unstyled-list;\n\n    margin-bottom: $site-margins;\n\n    li {\n        margin-bottom: 1rem;\n    }\n\n    .bbg__article-sidebar__list-label {\n        font-weight: bold;\n    }\n}\n\n\n@include media($medium-large-screen) {\n    .bbg__article-content {\n        float: left;\n        margin: {\n            top: 0;\n            right: auto;\n            bottom: $site-margins;\n            left: auto;\n        }\n        width: 56%;\n\n        >* {\n            max-width: none;\n        }\n\n        p {\n            margin: {\n                right: 0;\n                left: 0;\n            }\n\n            strong {\n                font-family: $font-sans;\n            }\n        }\n\n        &.large {\n            border-right: $border-dotted;\n            margin-left: 0;\n            padding-right: 2%;\n            width: 65%;\n\n\n            /* Make the first paragraph larger */\n            .bbg__profile__content p:first-child {\n                font: {\n                    size: 1.7rem;\n                    weight: 100;\n                }\n                ;\n                line-height: 2.5;\n                margin: {\n                    top: -.8rem;\n                    bottom: $site-margins;\n                }\n            }\n        }\n    }\n\n\n    .bbg__article-sidebar {\n        float: right;\n        font-family: $font-sans; //max-width: 17rem;\n        width: 30%;\n\n        p, li {line-height: 1.5;}\n\n        &.large {\n            max-width: none;\n            width: 33%;\n\n            &:first-child {\n                margin-top: -1rem;\n            }\n        }\n    }\n\n    /* Used for private author guide pages */\n    /* Flips the sidebar to the left */\n    /*\n\t.status-private {\n\t\t.bbg__article-content.large{\n\t\t\tborder-right: none;\n\t\t\tborder-left: $border-dotted;\n\t\t\tfloat: right;\n\t\t\tmargin-left: auto;\n\t\t\tpadding: {\n\t\t\t\tright: 0;\n\t\t\t\tleft: 2%;\n\t\t\t}\n\t\t}\n\t\t.bbg__article-sidebar.large {\n\t\t\tfloat: left;\n\t\t}\n\t}\n\t*/\n}\n\n.bbg__profile__content__section {\n    margin: {\n        bottom: $site-margins;\n    }\n}\n\n.bbg__article-sidebar--left {\n    border-bottom: $border-solid;\n    margin: 0 auto 3rem auto;\n    max-width: 600px;\n    padding-bottom: $site-margins;\n\n    @include media($medium-large-screen) {\n        border-bottom: none;\n        display: inline-block;\n        float: left;\n        margin-top: 0;\n        margin-right: $site-margins;\n        /*width: 20%;*/\n        width: 6%;\n    }\n}\n\n/*******************************/\n/*    CH 03  FOOTER STYLES    */\n\n.bbg__footer {\n    position: relative;\n    z-index: 9999;\n}\n\n.bbg__footer__big-top {\n    padding: 0 $site-margins;\n}\n\n.bbg-continue {\n    a {\n        font: {\n            family: $font-sans;\n            weight: 700;\n        }\n        text-transform: uppercase;\n        font-size: .9em;\n    }\n}\n\n.bbg__foot